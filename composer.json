{
    "name": "tetsuobe/eventstore-client",
    "description": "EventStore client for PHP",
    "type": "library",
    "keywords": [
        "eventsourcing",
        "eventstore"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Bellettini",
            "email": "davide@bellettini.me",
            "homepage": "http://bellettini.me/"
        },
        {
            "name": "NicolÃ² Pignatelli",
            "email": "pignatelli.nicolo@gmail.com",
            "homepage": "http://nicolopignatelli.me/"
        },
        {
            "name": "Remigiusz Specht",
            "email": "tetsuobe@gmail.com",
            "homepage": "http://nicolopignatelli.me/"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/dbellettini/php-eventstore-client/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/psr7": "~1.2",
        "ramsey/uuid": "~3.2",
        "marc-mabe/php-enum": "~2.2"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "~6.1",
        "phpunit/phpunit": "~5.2",
        "kevinrob/guzzle-cache-middleware": "~1.1",
        "doctrine/cache": "~1.6"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Default PSR-7 implementation",
        "kevinrob/guzzle-cache-middleware": "Cache HTTP requests",
        "doctrine/cache": "Use Doctrine to cache HTTP requests",
        "tetsuobe/eventstore-client-bundle": "Integrate Event Store with Symfony 2 and Symfony 3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "EventStore": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "EventStore": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6.x-dev"
        }
    }
}
